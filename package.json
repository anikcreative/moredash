{
  "name": "moredash",
  "version": "1.0.0",
  "description": "A general-purpose utility library for when lodash isn't enough",
  "main": "lib/index.ts",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "prebuild": "npm-run-all --serial typecheck lint test:coverage clean",
    "clean": "rimraf lib",
    "build:babel": "babel ./src -d ./lib --source-maps --extensions '.ts' --ignore '**/*.spec.ts'",
    "build:ts": "tsc --project ./tsconfig.prod.json",
    "build": "npm-run-all --serial build:babel build:ts",
    "typecheck": "tsc",
    "test": "jest src --verbose",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src --ext .ts,.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrhart08/moredash.git"
  },
  "keywords": [
    "utility"
  ],
  "author": "Joseph Hart",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jrhart08/moredash/issues"
  },
  "homepage": "https://github.com/jrhart08/moredash#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-lodash": "^7.2.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
